import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		//get input
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		System.out.print("0");
		for(int i=2;i<=a;i++){
		    if(i%2==0){
		        System.out.print(" "+((i*i)-2));
		    }else{
		        System.out.print(" "+((i*i)-1));
		    }
		}
	}
}
